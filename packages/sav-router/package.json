{
  "name": "sav-router",
  "version": "1.0.13",
  "description": "another nodejs controller style router",
  "main": "dist/sav-router.cjs.js",
  "module": "dist/sav-router.es.js",
  "jsnext:main": "dist/sav-router.es.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "standard rollup.config.js test/** src/**",
    "start": "watch -p \"+(test|src)/*.js\" -c \"npm run lint -- --fix && npm run na\"",
    "na": "nyc ava ./test",
    "pretest": "npm run lint",
    "test": "npm run na",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savjs/sav-router.git"
  },
  "author": {
    "name": "jetiny",
    "email": "86287344@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savjs/sav-router/issues"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "chai": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "nyc": "^10.0.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "standard": "^9.0.1",
    "watch-cli": "^0.2.1"
  },
  "dependencies": {
    "koa-compose": "^3.2.1",
    "path-to-regexp": "^1.7.0",
    "sav-decorator": "^1.0.6",
    "sav-util": "^1.0.14"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "test"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-decorators-legacy",
      "transform-es2015-modules-commonjs"
    ]
  },
  "homepage": "https://github.com/savjs/sav-router#readme"
}